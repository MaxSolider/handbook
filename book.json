{
    "title": "MaxSolider's HandBook",
    "author": "MaxSolider",
    "description": "select * from learn",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "styles": {
        "website": "./styles/website.css"
    },
    "structure": {
        "readme": "README.md"
    },
    "links": {
        "sidebar": {
            "MaxSolider's Handbook": "https://github.com/MaxSolider"
        }
    },
    "plugins": [
        "-sharing",
        "splitter",
        "expandable-chapters-small",
        "anchors",

        "github",
        "github-buttons",
        "donate",
        "sharing-plus",
        "anchor-navigation-ex",
        "favicon",
	"theme-comscore",
	"insert-logo"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/MaxSolider"
        },
        "github-buttons": {
            "buttons": [{
                "user": "MaxSolider",
                "repo": "handbook",
                "type": "star",
                "size": "small",
                "count": true
                }
            ]
        },
        "donate": {
            "alipay": "/source/images/donate.png",
            "title": "",
            "button": "赞赏",
            "alipayText": " "
        },
        "anchor-navigation-ex": {
            "showLevel": true
        },
        "favicon":{
            "shortcut": "/source/images/favicon.ico",
            "bookmark": "/source/images/favicon.ico",
            "appleTouch": "/source/images/favicon.png",
            "appleTouchMore": {
                "120x120": "/source/images/favicon.png",
                "180x180": "/source/images/favicon.png"
            }
        },
	"theme-default": {
		"showLevel": true
	},
	"insert-logo": {
		"url": "/source/images/logo.png",
        	"style": "background: none; max-height: 300px; min-height: 100"
	}
    }
}
